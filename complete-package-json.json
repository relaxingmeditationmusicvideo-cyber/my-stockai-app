{
  "name": "stock-analytics-pro",
  "version": "1.0.0",
  "description": "Professional Stock Analytics Dashboard with Real-time Market Data",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm install --production",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docker:build": "docker build -t stock-analytics-pro .",
    "docker:run": "docker run -p 3000:3000 stock-analytics-pro",
    "deploy:heroku": "git push heroku main",
    "setup": "npm install && cp .env.example .env"
  },
  "keywords": [
    "stock",
    "analytics",
    "finance",
    "real-time",
    "market-data",
    "trading",
    "dashboard"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.5.0",
    "ws": "^8.14.2",
    "redis": "^4.6.8",
    "node-cron": "^3.0.2",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "compression": "^1.7.4",
    "winston": "^3.10.0",
    "morgan": "^1.10.0",
    "joi": "^17.9.2",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.4",
    "uuid": "^9.0.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.2",
    "@types/node": "^20.5.9",
    "concurrently": "^8.2.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/stock-analytics-pro.git"
  },
  "bugs": {
